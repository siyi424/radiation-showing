{"ast":null,"code":"import ChartRadi from './components/ChartRadi.vue';\nimport DataRadi from './components/DataRadi.vue'; // 引入数据\n\nimport DataList from '../public/DataList.json';\nexport default {\n  components: {\n    ChartRadi,\n    DataRadi\n  },\n  name: 'app',\n\n  data() {\n    return {\n      DataList\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,QAAP,MAAqB,2BAArB,C,CACA;;AACA,OAAOC,QAAP,MAAqB,yBAArB;AAGA,eAAe;EACbC,UAAU,EAAE;IAAEH,SAAF;IAAaC;EAAb,CADC;EAEbG,IAAI,EAAE,KAFO;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLH;IADK,CAAP;EAGF;;AAPa,CAAf","names":["ChartRadi","DataRadi","DataList","components","name","data"],"sourceRoot":"","sources":["/home/siyi/Projects/radi-demo/radi-showing/src/App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"header\">\n      <h1 class=\"title-center\">监测数据显示屏</h1>\n    </div>\n\n    <!-- 中间body部分 -->\n    <div class=\"body\">\n      <div id=\"chart\"><chart-radi></chart-radi></div>\n      <div id=\"dataset\"><data-radi :dataset=\"DataList\"></data-radi></div>\n    </div>\n\n\n    <!-- 结尾展示小组成员 -->\n    <div class=\"footer\">\n      <div>\n        小组成员：韩立荟、胡伟、黄思怡 \n      </div>\n      <div>\n        科研训练题目：智能化个人辐射剂量率检测系统研究\n      </div>\n\n      <div id=\"footer-right\">\n        <img src=\"./assets/logo.png\">\n      </div>\n\n    </div>\n    \n  </div>\n</template>\n\n\n\n<script>\nimport ChartRadi from './components/ChartRadi.vue'\nimport DataRadi from './components/DataRadi.vue'\n// 引入数据\nimport DataList from '../public/DataList.json'\n\n\nexport default {\n  components: { ChartRadi, DataRadi},\n  name: 'app',\n  data() {\n    return {\n      DataList\n    }\n  }\n}\n</script>\n\n\n\n\n<style>\n* {\n  font-family: Microsoft YaHei;\n  box-sizing: border-box;\n  padding: 0;\n  margin: 0;\n}\n\n#app {\n  background-image: radial-gradient( circle farthest-corner at 10% 20%,  rgba(90,92,106,1) 0%, rgba(32,45,58,1) 81.3% );\n  display: flex;\n  flex-direction: column;\n  height: 100vh;\n  color: whitesmoke;\n}\n\n/* animate */\n@keyframes fadeInDown {\n  from {\n    opacity: 0;\n    transform: translate3d(0, -100%, 0);\n  }\n\n  to {\n    opacity: 1;\n    transform: translate3d(0, 0, 0);\n  }\n}\n\n\n@keyframes fadeInLeft {\n  from {\n    opacity: 0;\n    transform: translate3d(-100%, 0, 0);\n  }\n\n  to {\n    opacity: 1;\n    transform: translate3d(0, 0, 0);\n  }\n}\n\n\n@keyframes fadeInRight {\n  from {\n    opacity: 0;\n    transform: translate3d(100%, 0, 0);\n  }\n\n  to {\n    opacity: 1;\n    transform: translate3d(0, 0, 0);\n  }\n}\n\n@keyframes fadeInUp {\n  from {\n    opacity: 0;\n    transform: translate3d(0, 100%, 0);\n  }\n\n  to {\n    opacity: 1;\n    transform: translate3d(0, 0, 0);\n  }\n}\n\n\n\n/* header */\n.header {\n  flex: 1;\n  animation-name: fadeInDown;\n  animation-duration: 2s;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.title-center {\n  text-align: center;\n  color:whitesmoke;\n}\n\n\n/* body */\n.body {\n  flex: 11;\n  display: flex;\n  flex-direction: row;\n  align-items: stretch;\n  padding: 20px 40px;\n}\n\n#chart {\n  background: rgba(225,225,225,0.9);\n  flex: 1;\n  animation-name: fadeInLeft;\n  animation-duration: 2s;\n  margin-right: 20px;\n}\n\n#dataset {\n  flex: 1;\n  animation-name: fadeInRight;\n  animation-duration: 2s;\n  margin-left: 20px;\n}\n\n\n/* footer */\n.footer {\n  flex: 2;\n  display: flex;\n  max-height: 14.4%;\n  justify-content: space-between;\n  align-items: center;\n  animation-name: fadeInUp;\n  animation-duration: 2s;\n  padding: 0 40px;\n\n}\n\n#footer-right img{\n  align-items: stretch;\n}\n</style>"]},"metadata":{},"sourceType":"module"}