ion: row;\n  align-items: stretch;\n  padding: 20px 40px;\n}\n\n#chart {\n  background: rgba(225,225,225,0.9);\n  flex: 1;\n  animation-name: fadeInLeft;\n  animation-duration: 2s;\n  margin-right: 20px;\n}\n\n#dataset {\n  flex: 1;\n  animation-name: fadeInRight;\n  animation-duration: 2s;\n  margin-left: 20px;\n}\n\n\n/* footer */\n.footer {\n  flex: 2;\n  display: flex;\n  max-height: 14.4%;\n  justify-content: space-between;\n  align-items: center;\n  animation-name: fadeInUp;\n  animation-duration: 2s;\n  padding: 0 40px;\n\n}\n\n#footer-right img{\n  align-items: stretch;\n}\n</style>"]},"metadata":{},"sourceType":"module"}